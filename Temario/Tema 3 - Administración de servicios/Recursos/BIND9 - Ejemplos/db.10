;
; BIND reverse data file
;
$TTL	604800
@	IN	SOA	ejemplo.uma. root.ejemplo.uma. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	dns.ejemplo.uma.
15	IN	PTR	ejemplo.uma.
15	IN	PTR	www.ejemplo.uma.
16	IN	PTR	dns.ejemplo.uma.
16	IN	PTR	mail.ejemplo.uma.
